{
  "name": "idom",
  "version": "0.3.1",
  "description": "Write functional view components with incremental DOM render",
  "main": "lib/index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --config webpack.config.js",
    "dev": "webpack-dev-server --inline --config webpack.config.js",
    "lib": "mkdir -p lib && babel src --out-dir lib",
    "prepublish": "npm run lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "incremental-dom": "^0.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "idom"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/eliot-akira/idom.git"
  },
  "keywords": [],
  "license": "MIT"
}
