{
  "name": "idom",
  "version": "0.0.4",
  "description": "Smart-rendering views with JSX and incremental DOM",
  "main": "lib/index.js",
  "scripts": {
    "build": "mkdir -p dist && browserify src/index.js -t babelify -p [minifyify --no-map] -o dist/idom.js",
    "build:lib": "babel src --out-dir lib",
    "prepublish": "npm run build:lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "incremental-dom": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "minifyify": "^7.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/eliot-akira/idom.git"
  },
  "keywords": [],
  "license": "MIT"
}
